    "start": "nx serve",
    "build": "nx build",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.+(js|ts)\"",
    "db:migration": "cd apps/backend && npx mikro-orm migration:up && cd ../code-crawler && npx mikro-orm migration:up && cd ../scm-connector && npx mikro-orm migration:up && cd ../sca && npx mikro-orm migration:up && cd ../finding && npx mikro-orm migration:up && cd ../til && npx mikro-orm migration:up && cd ../repository && npx mikro-orm migration:up",
    "db:migration:create": "cd apps/backend && npx mikro-orm migration:create && cd ../code-crawler && npx mikro-orm migration:create && cd ../scm-connector && npx mikro-orm migration:create && cd ../finding && npx mikro-orm migration:create && cd ../til && npx mikro-orm migration:create && cd ../repository && npx mikro-orm migration:create && cd ../sca && npx mikro-orm migration:create",
    "test": "nx test",
    "test:e2e": "nx test e2e-tester",
    "backend": "nx serve backend --host=0.0.0.0 --port=9231",
    "backend:build": "nx run backend:build:production",
    "crawler": "nx serve code-crawler --host=0.0.0.0 --port=9232",
    "crawler:build": "nx run code-crawler:build:production",
    "coda": "nx serve code-analysis --host=0.0.0.0 --port=9236",
    "coda:build": "nx run code-analysis:build:production",
    "coda:standalone": "nx serve-local code-analysis --host=0.0.0.0 --port=9237",
    "coda:standalone:ts-node": "ts-node --project apps/code-analysis/tsconfig.ts-node.json apps/code-analysis/src/run.local.ts dotenv_config_path='./apps/code-analysis/.env'",
    "coda:standalone-ng:ts-node-no-env-file": "TS_NODE_PROJECT='apps/code-analysis/tsconfig.ts-node.json' node -r ts-node/register --expose-gc --compact-on-every-full-gc apps/code-analysis/src/run.local.ts",
    "coda:tree-sitter-ganerator": "ts-node --project apps/code-analysis/tsconfig.ts-node.json apps/code-analysis/src/utils/syntax.kinds.generator.ts",
    "coda:test": "nx run code-analysis:test",
    "frontend": "nx serve frontend --host=0.0.0.0",
    "frontend:build": "nx run frontend:build:production",
    "connector": "nx serve scm-connector --host=0.0.0.0 --port=9233",
    "connector:build": "nx run scm-connector:build:production",
    "sca": "nx serve sca --host=0.0.0.0 --port=9235",
    "sca:build": "nx run sca:build:production",
    "til": "nx serve til --host=0.0.0.0 --port=9239",
    "til:build": "nx run til:build:production",
